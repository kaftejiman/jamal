{
  	"name": "jamal",
  	"extensionDependencies": [
  	  	"tintinweb.graphviz-interactive-preview"
  	],
  	"displayName": "Jamal",
  	"description": "Android applications binary analysis framework.",
  	"version": "0.3.1",
  	"icon": "icon.png",
  	"license": "See license in LICENSE.md",
  	"publisher": "kaftejiman",
  	"repository": "https://github.com/kaftejiman/jamal",
  	"keywords": [
  	  	"Android",
  	  	"APK",
  	  	"Mobile",
  	  	"Decompiler",
  	  	"Reverse Engineering"
  	],
  	"engines": {
  	  	"vscode": "^1.52.0"
  	},
  	"categories": [
  	  	"Programming Languages",
  	  	"Formatters",
  	  	"Snippets",
  	  	"Other"
  	],
  	"activationEvents": [
  	  	"onLanguage:soot",
  	  	"onCommand:jamal.runAnalysis",
  	  	"onCommand:jamal.previewcfg"
  	],
  	"main": "./src/extension.js",
  	"contributes": {
  	  	"languages": [
  	  	  	{
  	  	  	  	"id": "soot",
  	  	  	  	"aliases": [
  	  	  	  	  	"Soot IR",
  	  	  	  	  	"Jimple",
  	  	  	  	  	"Grimple"
  	  	  	  	],
  	  	  	  	"extensions": [
  	  	  	  	  	".jimple",
  	  	  	  	  	".grimple"
  	  	  	  	],
  	  	  	  	"configuration": "./language-configuration.json"
  	  	  	},
  	  	  	{
  	  	  	  	"id": "soot-dava",
  	  	  	  	"aliases": [
  	  	  	  	  	"Dava"
  	  	  	  	],
  	  	  	  	"extensions": [
  	  	  	  	  	".dava"
  	  	  	  	],
  	  	  	  	"configuration": "./language-configuration.json"
  	  	  	},
  	  	  	{
  	  	  	  	"id": "soot-jasmin",
  	  	  	  	"aliases": [
  	  	  	  	  	"Jasmin"
  	  	  	  	],
  	  	  	  	"extensions": [
  	  	  	  	  	".jasmin"
  	  	  	  	],
  	  	  	  	"configuration": "./language-configuration.json"
  	  	  	}
  	  	],
  	  	"grammars": [
  	  	  	{
  	  	  	  	"language": "soot",
  	  	  	  	"scopeName": "source.jimple",
  	  	  	  	"path": "./syntaxes/jimple.tmLanguage.json"
  	  	  	},
  	  	  	{
  	  	  	  	"language": "soot-dava",
  	  	  	  	"scopeName": "source.dava",
  	  	  	  	"path": "./syntaxes/dava.tmLanguage.json"
  	  	  	},
  	  	  	{
  	  	  	  	"language": "soot-jasmin",
  	  	  	  	"scopeName": "source.jasmin",
  	  	  	  	"path": "./syntaxes/jasmin.tmLanguage.json"
  	  	  	}
  	  	],
  	  	"commands": [
  	  	  	{
  	  	  	  	"command": "jamal.runAnalysis",
  	  	  	  	"title": "JAMAL: Run Analysis"
  	  	  	},
  	  	  	{
  	  	  	  	"command": "jamal.previewcfg",
  	  	  	  	"title": "Open CFG Preview to the side"
  	  	  	}
  	  	],
  	  	"menus": {
  	  	  	"explorer/context": [
  	  	  	  	{
  	  	  	  	  	"when": "resourceExtname == .apk",
  	  	  	  	  	"command": "jamal.runAnalysis",
  	  	  	  	  	"group": "navigation@+1"
  	  	  	  	}
  	  	  	],
  	  	  	"editor/context": [
  	  	  	  	{
  	  	  	  	  	"command": "jamal.previewcfg",
  	  	  	  	  	"group": "navigation@1",
  	  	  	  	  	"when": "resourceExtname == .jimple || resourceExtname == .grimple"
  	  	  	  	}
  	  	  	]
  	  	},
		"configuration": [{
				"title": "Jamal parameters",
				"properties": {
					"jamal.java_path": {
						"type": "string",
						"description": "Path to Java binary",
						"default": ""
					},
					"jamal.androids_path": {
						"type": "string",
						"description": "Path to android-platforms",
						"default":""
					},
					"jamal.deps_path":{
						"type":"string",
						"description": "Path to the engines dependencies",
						"default":""
					},
					"jamal.java_memory_limit":{
						"type":"string",
						"description": "Java memory limit",
						"default":"2G"
					}				
				}
		}]
	},
  	"scripts": {
  	  	"lint": "eslint src/",
  	  	"pretest": "yarn run lint",
  	  	"test": "node ./test/runTest.js",
  	  	"webpack": "webpack --mode development",
  	  	"webpack-dev": "webpack --mode development --watch",
  	  	"test-compile": "tsc -p ./"
  	},
  	"devDependencies": {
  	  	"@types/glob": "^7.1.3",
  	  	"@types/mocha": "^8.0.4",
  	  	"@types/node": "^12.11.7",
  	  	"@types/vscode": "^1.52.0",
  	  	"eslint": "^7.15.0",
  	  	"glob": "^7.1.6",
  	  	"mocha": "^8.1.3",
  	  	"typescript": "^4.1.2",
  	  	"vscode-test": "^1.4.1"
  	},
  	"dependencies": {
  	  	"@particle/fetch-file": "^2.0.1",
  	  	"decompress": "^4.2.1",
  	  	"node-stream-zip": "^1.13.1",
  	  	"xml2js": "^0.4.23"
  	}
}
