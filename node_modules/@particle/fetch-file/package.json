{
  "name": "@particle/fetch-file",
  "description": "Download a file, report progress, retry when appropriate, and verify integrity of downloaded bits",
  "version": "2.0.1",
  "author": "Particle Industries, inc",
  "license": "UNLICENSED",
  "main": "src/index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "version": "npm run changelog:update",
    "clean": "npm run clean:modules",
    "clean:modules": "rm -rf ./node_modules",
    "test": "npm run test:unit",
    "test:ci": "npm run test:unit -- --forbid-only && npm run coverage",
    "test:unit": "mocha './src/**/*.test.js' --color",
    "test:unit:watch": "npm run test:unit -- --watch",
    "test:unit:inspect": "npm run test:unit -- --inspect-brk",
    "test:unit:silent": "PARTICLE_NOOP=$(npm run test:unit)",
    "coverage": "nyc --reporter=text --include='src/**/*.js' --temp-dir=./tmp/ --check-coverage --lines 100 --all npm run test:unit:silent",
    "changelog:update": "node ../../bin/update-changelog.js fetch-file",
    "docs:build": "node ../../bin/build-docs.js fetch-file"
  },
  "dependencies": {
    "abort-controller": "^3.0.0",
    "node-fetch": "^2.6.1",
    "progress-stream": "^2.0.0"
  },
  "engines": {
    "node": ">=10",
    "npm": "6.x"
  },
  "gitHead": "f2720b9e9c86c032f5205fca1569131ac8b0d69d"
}
